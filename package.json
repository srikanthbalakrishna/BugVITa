{
  "name": "bugvita",
  "version": "1.0.0",
  "description": "BugVITa bug tracking system",
  "main": "index.js",
  "scripts": {
    "old-client-install": "npm install --prefix client-old",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "old-client": "npm start --prefix client-old",
    "dev-old-client": "concurrently \"npm run server\" \"npm run old-client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client-old && npm run build --prefix client-old"
  },
  "author": "srikanth",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.2",
    "express": "^4.17.1",
    "mongoose": "^5.12.7",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
